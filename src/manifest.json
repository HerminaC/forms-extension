{
  "manifest_version": 2,
  "name": "French Holiday Form Generator",
  "description": "Used by french employees to generate their holiday form",
  "author": "Julien Wajsberg",
  "license": "MIT",
  "version": "1.3.3",
  "applications": {
    "gecko": {
      "id": "jid1-Ie0ShncwSkPKJg@jetpack",
      "strict_min_version": "55.0",
      "update_url": "https://julienw.github.io/forms-extension/update.xml"
    }
  },
  "homepage_url": "https://github.com/julienw/forms-extension",
  "browser_action": {
    "default_icon": "data/icon-opt.svg",
    "default_title": "Generate forms",
    "browser_style": true
  },
  "background": {
    "scripts": ["lib/main.js"]
  },
  "permissions": [
    "https://pto.mozilla.org/mypto.php",
    "tabs"
  ]
}
